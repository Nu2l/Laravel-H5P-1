{
    "name": "nu2l/Laravel-H5P-1",
    "description": "H5P Plugin for laravel platform",
    "type": "library",
    "authors": [
        {
            "name": "Abdelouahab Djoudi",
            "email": "info@djoudi.net"
        },
        
        {
            "name": "Vittorio Emmermann",
            "email": "vittorio@cierra.eu"
